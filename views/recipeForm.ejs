<div class="panel panel-default">
	<div class="panel-heading">
    	<h3 class="panel-title">Recipe Generation</h3>
  	</div>
  <div class="panel-body">
 		<input type="hidden" id="selectedXmlFileName" name="selectedXmlFileName" />
 		<input type="hidden" id="overWriteRecipeXmlId" name="overWriteRecipeXmlId" />
 		<div class="dropdown">
  			<button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true">
    		Select
    			<span class="caret"></span>
  			</button>
  		<ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
    		<% for(var i=0; i<item.length; i++) {%>
    			<li role="presentation"><a role="menuitem" tabindex="-1" href="<%= item[i].ID %>"><%= item[i].FILENAME %></a></li>
    		<% } %>
  		</ul>
	</div>
	<div class = "recipe-button">  
		<a class="btn btn-primary" href="#" role="button">Load</a>
	</div>
	
	<a class="btn btn-primary btn-newRecipe" href="#" role="button" id="newRecipe">New</a>
	<div class = "fileSubmit">
		<form name="recipeGenerationForm" method="get" action="/saveRecipe">
			<input style="width: 200px;" type="text" name="recipeFileName" id="recipeFileName" class="form-control" placeholder="File Name"/>
			<a class="btn btn-primary" href="#" role="button">Submit</a>
			
		</form>
	</div>
  </div><!-- panel-body-->
</div>